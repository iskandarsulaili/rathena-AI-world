{
    "test_prompts": {
        "balance_agent": {
            "class_analysis": {
                "prompt": "Analyze class performance data for Job ID {job_id} with win rate {win_rate} and usage rate {usage_rate}",
                "variables": {
                    "job_id": ["1", "4", "7"],
                    "win_rate": ["0.45", "0.55", "0.65"],
                    "usage_rate": ["0.2", "0.3", "0.4"]
                }
            },
            "skill_balance": {
                "prompt": "Evaluate skill ID {skill_id} with success rate {success_rate} and damage contribution {damage_contrib}",
                "variables": {
                    "skill_id": ["100", "200", "300"],
                    "success_rate": ["0.75", "0.85", "0.95"],
                    "damage_contrib": ["0.3", "0.4", "0.5"]
                }
            }
        },
        "reward_agent": {
            "reward_generation": {
                "prompt": "Generate reward for player with play time {play_time} hours and achievement count {achievements}",
                "variables": {
                    "play_time": ["10", "50", "100"],
                    "achievements": ["5", "15", "30"]
                }
            },
            "dopamine_control": {
                "prompt": "Calculate dopamine impact for reward type {reward_type} with current level {current_level}",
                "variables": {
                    "reward_type": ["item", "exp", "status"],
                    "current_level": ["0.3", "0.5", "0.7"]
                }
            }
        },
        "monster_agent": {
            "behavior_analysis": {
                "prompt": "Design monster behavior for ID {monster_id} with difficulty {difficulty} in environment {environment}",
                "variables": {
                    "monster_id": ["1001", "1002", "1003"],
                    "difficulty": ["easy", "medium", "hard"],
                    "environment": ["dungeon", "field", "castle"]
                }
            },
            "group_tactics": {
                "prompt": "Create group tactics for monster type {monster_type} with group size {group_size}",
                "variables": {
                    "monster_type": ["undead", "demon", "dragon"],
                    "group_size": ["3", "5", "7"]
                }
            }
        },
        "social_agent": {
            "community_analysis": {
                "prompt": "Analyze community interaction pattern {pattern_id} with activity level {activity}",
                "variables": {
                    "pattern_id": ["guild", "party", "trade"],
                    "activity": ["low", "medium", "high"]
                }
            },
            "event_planning": {
                "prompt": "Plan community event for {participant_count} players with focus on {event_type}",
                "variables": {
                    "participant_count": ["10", "50", "100"],
                    "event_type": ["pvp", "pve", "social"]
                }
            }
        },
        "quest_agent": {
            "quest_generation": {
                "prompt": "Generate quest for level {level} player with {playstyle} playstyle",
                "variables": {
                    "level": ["30", "60", "90"],
                    "playstyle": ["casual", "moderate", "hardcore"]
                }
            },
            "difficulty_scaling": {
                "prompt": "Scale quest difficulty for party size {party_size} with average level {avg_level}",
                "variables": {
                    "party_size": ["1", "3", "6"],
                    "avg_level": ["40", "70", "100"]
                }
            }
        }
    },
    "test_contexts": {
        "player": {
            "new_player": {
                "play_time": "10",
                "level": "20",
                "achievements": "5",
                "playstyle": "casual"
            },
            "mid_game": {
                "play_time": "100",
                "level": "60",
                "achievements": "25",
                "playstyle": "moderate"
            },
            "veteran": {
                "play_time": "500",
                "level": "99",
                "achievements": "50",
                "playstyle": "hardcore"
            }
        },
        "environment": {
            "low_activity": {
                "player_count": "50",
                "time_of_day": "off-peak",
                "server_load": "low"
            },
            "normal_activity": {
                "player_count": "200",
                "time_of_day": "normal",
                "server_load": "medium"
            },
            "high_activity": {
                "player_count": "500",
                "time_of_day": "peak",
                "server_load": "high"
            }
        }
    }
}